{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run --browser chrome",
    "e2e:run:spec": "cypress run --browser chrome --spec",
