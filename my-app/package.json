{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "delete:reports": "rimraf cypress/results || exit 0",
    "prereport": "npm run delete:reports",
    "e2e:open": "cypress open",
    "e2e:run:spec": "cypress run --browser chrome --spec",
    "mochawesome:merge": "npx mochawesome-merge cypress/results/mochawesome/*.json -o mochawesome.json && npx marge mochawesome.json --inlineAssets",
    "junit:merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "e2e:run": "npm run prereport && cypress run --browser chrome && npm run mochawesome:merge && npm run junit:merge",
    "e2e:run:staging": " npm run e2e:run --config-file staging.config.js && npm run mochawesome:merge && npm run junit:merge",
    "e2e:run:tags:dev": "npm run prereport && cypress run --browser chrome --env tags=\"@dev\" && npm run mochawesome:merge && npm run junit:merge",
    "e2e:run:tags:regression": "npm run prereport && cypress run --browser chrome --env tags=\"@regression and @dev\" && npm run mochawesome:merge && npm run junit:merge"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@hookform/resolvers": "^4.1.0",
    "@next/env": "^15.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.474.0",
    "multiple-cucumber-html-reporter": "^3.9.0",
    "next": "15.1.6",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sass": "^1.84.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.0.3",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^2.0.5",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
